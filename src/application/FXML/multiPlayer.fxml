<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>



<BorderPane fx:id="configuration" minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.MultiPlayerController">
    <top>
        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="TOP_CENTER" prefHeight="40.0" prefWidth="200.0">
                    <children>
                        <Region fx:id="easy" id="easyChoice" maxHeight="30.0" minHeight="30.0" minWidth="40.0" onMouseClicked="#easy" prefHeight="40.0" prefWidth="60.0">
                            <HBox.margin>
                                <Insets left="40.0" />
                            </HBox.margin></Region>
                        <Region fx:id="medium" id="mediumChoice" onMouseClicked="#medium" layoutX="380.0" layoutY="10.0" maxHeight="30.0" minHeight="30.0" minWidth="40.0" prefHeight="40.0" prefWidth="60.0">
                            <HBox.margin>
                                <Insets left="10.0" />
                            </HBox.margin>
                        </Region>
                        <Region fx:id="hard" id="hardChoice" onMouseClicked="#hard" layoutX="410.0" layoutY="10.0" maxHeight="30.0" minHeight="30.0" minWidth="40.0" prefHeight="40.0" prefWidth="60.0">
                            <HBox.margin>
                                <Insets left="10.0" />
                            </HBox.margin>
                        </Region>
                    </children>
                    <VBox.margin>
                        <Insets top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
                    <VBox.margin>
                        <Insets top="15.0" />
                    </VBox.margin>
                    <children>
                        <Region fx:id="donkey" id="Fdonkey" onMouseClicked="#donkey" maxHeight="30.0" minHeight="30.0" minWidth="40.0" prefHeight="40.0" prefWidth="60.0">
                            <HBox.margin>
                                <Insets left="40.0" />
                            </HBox.margin>
                        </Region>
                        <Region fx:id="horse" id="Fhorse" onMouseClicked="#horse" maxHeight="30.0" minHeight="30.0" minWidth="40.0" prefHeight="40.0" prefWidth="60.0">
                            <HBox.margin>
                                <Insets left="10.0" />
                            </HBox.margin>
                        </Region>
                        <Region fx:id="unicorn" id="Funicorn" onMouseClicked="#unicorn" layoutX="435.0" layoutY="10.0" maxHeight="30.0" minHeight="30.0" minWidth="40.0" prefHeight="40.0" prefWidth="60.0">
                            <HBox.margin>
                                <Insets left="10.0" />
                            </HBox.margin>
                        </Region>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Region fx:id="speed" maxHeight="39.0" maxWidth="180.0" minHeight="39.0" minWidth="180.0" nodeOrientation="LEFT_TO_RIGHT">
                            <HBox.margin>
                                <Insets left="20.0" />
                            </HBox.margin></Region>
                        <JFXButton maxHeight="10.0" maxWidth="10.0" minHeight="20.0" minWidth="20.0">
                            <HBox.margin>
                                <Insets bottom="-15.0" left="25.0" />
                            </HBox.margin>
                            <font>
                                <Font size="10.0" />
                            </font></JFXButton>
                    </children>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Region fx:id="specialAbility" maxHeight="38.0" maxWidth="180.0" minHeight="39.0" minWidth="180.0">
                            <HBox.margin>
                                <Insets left="20.0" />
                            </HBox.margin></Region>
                        <JFXButton maxHeight="10.0" maxWidth="10.0" minHeight="20.0" minWidth="20.0" text="Button">
                            <HBox.margin>
                                <Insets bottom="-15.0" left="25.0" />
                            </HBox.margin>
                            <font>
                                <Font size="10.0" />
                            </font></JFXButton>
                    </children>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <JFXButton onAction="#previousWeapon">
                            <HBox.margin>
                                <Insets bottom="50.0" right="33.0" />
                            </HBox.margin>
                        </JFXButton>
                        <Region fx:id="weapon" minHeight="151.0" minWidth="145.0" />
                        <JFXButton onAction="#nextWeapon">
                            <HBox.margin>
                                <Insets bottom="50.0" left="30.0" />
                            </HBox.margin>
                        </JFXButton>
                    </children>
                    <VBox.margin>
                        <Insets top="22.0" />
                    </VBox.margin>
                </HBox>
            </children>
            <BorderPane.margin>
                <Insets top="150.0" />
            </BorderPane.margin>
        </VBox>
    </top>
    <bottom>
        <HBox prefHeight="20.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <JFXButton fx:id="test" minHeight="35.0" minWidth="150.0" onAction="#loadGameOptions" text=" ">
                    <HBox.margin>
                        <Insets left="15.0" />
                    </HBox.margin></JFXButton>
                <Region maxHeight="100.0" prefHeight="37.0" prefWidth="600.0" />
                <JFXButton minHeight="35.0" minWidth="160.0" onAction="#launchGame" prefWidth="0.0" text=" ">
                    <HBox.margin>
                        <Insets right="15.0" />
                    </HBox.margin></JFXButton>
            </children>
            <BorderPane.margin>
                <Insets bottom="20.0" />
            </BorderPane.margin>
        </HBox>
    </bottom>
</BorderPane>

