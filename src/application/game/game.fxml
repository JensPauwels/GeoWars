<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65"
            fx:controller="application.game.GameScreenController">
    <center>
        <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
    </center>
    <bottom>
        <HBox prefHeight="30.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Button onAction="#goBack" mnemonicParsing="false" text="Go Back"/>
            </children>
        </HBox>
    </bottom>
    <top>
        <HBox prefHeight="50.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Label text="Highscore">
                    <HBox.margin>
                        <Insets top="50.0"/>
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
    </top>
</BorderPane>
